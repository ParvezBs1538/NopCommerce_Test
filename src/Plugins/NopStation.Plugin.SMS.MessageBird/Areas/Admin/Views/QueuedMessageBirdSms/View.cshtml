@model QueuedSmsModel

@{
    //page title
    ViewBag.PageTitle = T("Admin.NopStation.MessageBird.QueuedSmss.ViewDetails").Text;
    //active menu item (system name)
    NopHtml.SetActiveMenuItemSystemName("MessageBird.QueuedSmss");
}
<form asp-controller="QueuedMessageBirdSms" asp-action="View" method="post">
    <div class="content-header clearfix">
        <h1 class="float-left">
            @T("Admin.NopStation.MessageBird.QueuedSmss.ViewDetails")
            <small>
                <i class="fas fa-arrow-circle-left"></i>
                <a asp-action="List">@T("Admin.NopStation.MessageBird.QueuedSmss.BackToList")</a>
            </small>
        </h1>
        <div class="float-right">
            <span id="queuedSms-delete" class="btn bg-red">
                <i class="far fa-trash-alt"></i>
                @T("Admin.Common.Delete")
            </span>
        </div>
    </div>
    @await Html.PartialAsync("_CreateOrUpdate", Model)
</form>
<nop-delete-confirmation asp-model-id="@Model.Id" asp-button-id="queuedSms-delete" />
